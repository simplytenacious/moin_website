<!DOCTYPE html>
<!--suppress ALL -->
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{{ title }}{% endblock %}</title>
        {% block stylesheets %}
        <link href="/css/bootstrap.css" rel="stylesheet" />
        <link href="/css/main.css" rel="stylesheet" />
        {% endblock %}
    </head>
    <body>
        {% block body %}
            <div id="header">
                {% block menu %}{% include 'menu/menubar_head.html.twig' %}{% endblock %}
            </div>
            <div id="content">
                <div data-spy="scroll" data-target="#menubar_head" data-offset="0">
                    {% block main %}{% include 'pages/main.html.twig' %}{% endblock %}
                    {% block team %}{% include 'pages/team.html.twig' %}{% endblock %}
                    {% block games %}{% include 'pages/games.html.twig' %}{% endblock %}
                    {% block events %}{% include 'pages/events.html.twig' %}{% endblock %}
                </div>
            </div>
            <div id="footer">
                FOOTLOSE
            </div>
        {% endblock %}
        {% block javascripts %}
        <script type="text/javascript" src="/js/jquery.js"></script>
        <script type="text/javascript" src="/js/bootstrap.js"></script>
        <script type="text/javascript" src="/js/main.js"></script>
        {% endblock %}
    </body>
</html>
